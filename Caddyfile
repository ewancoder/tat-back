    reverse_proxy kartman:8080
}

kartman.typingrealm.com {
    reverse_proxy kartman-ui:80
}

api.dev.kartman.typingrealm.com {
    reverse_proxy ng-kartman-api:8080
}

dev.kartman.typingrealm.com {
    reverse_proxy ng-kartman-ui:80
}
